generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Blog {
  id        String   @id @default(cuid())
  title     String
  content   String   @db.Text
  imageUrl  String?
  createdAt DateTime @default(now())
}

model Analysis {
  id        String   @id @default(cuid())
  title     String
  fileUrl   String
  fileType  String
  createdAt DateTime @default(now())
}

